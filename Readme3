var style = {border: {color: '#FF0000', style: 'thin'},
				 pattern:{color: '#00FF00'}
				};
			const columns = [
				{
					width: 5,
					style: style,
					type:"number"
				},
				{
					width: 10
				},
				{width: 55,type:'date',style: {format: '[$-409]m/d/yy\ h:mm\ AM/PM;@'}},
			];
				
			const worksheetData = []
			for(var count = 0;count < 1;count++)
			{
				var data = [];
				data.push(count);
				data.push("US");
				var date = new Date();
				date.setFullYear(2015, Math.floor(Math.random() * 12), Math.floor(Math.random() * 27));
				date.setHours(Math.floor(Math.random()*23), Math.floor(Math.random()*59), Math.floor(Math.random()*59), 0);
				data.push(date);
				worksheetData.push(data);
			}
			
			workbook.addWorksheet()
				.setData(worksheetData)
				.setColumns(columns);
				
			workbook
				.save({type: 'blob'})
				.then(function (content) {
					saveAs(content, 'sample.xlsx');
				});
