this.util.addEvent(this.__txtSearch,"click",this.__txtSearchFocusHandler.bind(this));

NSMultiSelectDropdown.prototype.__txtSearchFocusHandler = function(event)
	{
		event = this.util.getEvent(event);
		event.stopPropagation();
	};
	
	.listCon .text
			{
				width: calc(100% - 15px);
			    white-space: nowrap;
			    overflow: hidden;
			    text-overflow: ellipsis;
			}
			var div = util.createDiv(null,"text");
	                div.appendChild(document.createTextNode(arrTexts[count]));
	                li.appendChild(div);
