NSUtil.prototype.findParent = function (element,parentTag,querySelector)
	{
		var retElement = null;
	    if(element)
	    {
	    	if(parentTag)
	    	{
	    		while(element) 
	    		{
	    			if(element.tagName && element.tagName.toLowerCase() == parentTag.toLowerCase()) 
	    			{
	    				retElement = element;
	    			    break;
	    			}
	    			element = element.parentElement;
	    		}
	    	}
	    	else if(querySelector)
	    	{
	    		while(element) 
	    		{
	    			if(element.matches(querySelector)) 
	    			{
	    				retElement = element;
	    			    break;
	    			}
	    			element = element.parentElement;
	    		}
	    	}
	    }
	    return retElement;
	};
  
  if (!Element.prototype.matches) 
	{
	    Element.prototype.matches = Element.prototype.msMatchesSelector;
	}
