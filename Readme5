var nsModuleExport = (function()
{
	var nsModuleExport = function(root,name,prototype)
	{
		if (typeof define === "function" && define.amd)
		{
			define(name, function () {return prototype;});
		}
		else if (typeof module !== 'undefined' && module.exports)
		{
			module.exports = prototype;
		}
		else
		{
			root[name] = prototype;
		}
	};
	return nsModuleExport;
})();
(function(root,name,prototype)
		{
		if (typeof define === "function" && define.amd)
		{
			define(name, function () {return prototype;});
		}
		else if (typeof module !== 'undefined' && module.exports)
		{
			module.exports = prototype;
		}
		else
		{
			root[name] = prototype;
		}
})(this,"nsModuleExport",nsModuleExport);

after getMaxZIndex

NSUtil.prototype.slideUp = function(element,duration,endCallback) 
 {
	 duration = duration || 10;
	 var counter = element.offsetHeight;
	 var subtractor = counter / 10;
	 element.style.overflow = "hidden";
 var interval = setInterval(function () 
 {
      counter -= subtractor;            
      if (counter > 0) 
      {
    	  element.style.height = counter + "px";
      } 
      else 
      {
    	  element.style.height = null;
    	  element.style.display = "none";
	    	  clearInterval(interval);
	    	  if(endCallback)
	    	  {
	    		  endCallback(element);
	    	  }
	      }
	  }, duration);
 };
 
 NSUtil.prototype.slideDown = function(element,duration,endCallback) 
 {
	 duration = duration || 10;
	 element.style.display = "";
 var height = element.offsetHeight;
 element.style.height = "0px";
 var adder = height / 10;
 var counter = 0;
 var interval = setInterval(function () 
 {            
      counter += adder;
      if (counter < height) 
      {
    	  element.style.height = counter + "px";
      } 
      else 
      {
    	  element.style.height = height + "px";
	    	  clearInterval(interval);
	    	  if(endCallback)
	    	  {
	    		  endCallback(element);
	    	  }
	      }
	  }, duration);
 };
