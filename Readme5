      var ajax = new NSAjax();
			ajax.post("/JSLib/hierarchicalNonFirstLevelData",{datalength: "10",parentid:item.id},{dataType:"json"}).then(
					function(data) 
					{
						console.log(data);
						if(data && data.length > 0)
						{
							nsGrid.addItemsAsChildren(item,data);
						}
						else
						{
							item.hasChildren = false;
							nsGrid.updateRowByIndex(rowIndex);
						}
					},
					function(error) 
					{ 
						/* handle an error */ 
					}
			);
      
